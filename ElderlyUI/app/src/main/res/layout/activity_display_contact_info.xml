<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="g1736229.elderlyui.DisplayContactInfo">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="5dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/email_button"
        android:layout_width="193dp"
        android:layout_height="46dp"
        android:onClick="emailPerson[MainActivity]"
        android:text="email"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="404dp" />

    <Button
        android:id="@+id/videocall_button"
        android:layout_width="193dp"
        android:layout_height="46dp"
        android:onClick="videoCallPerson[MainActivity]"
        android:text="video call"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="306dp" />

    <Button
        android:id="@+id/message_call"
        android:layout_width="193dp"
        android:layout_height="46dp"
        android:onClick="messagePerson[MainActivity]"
        android:text="message"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="211dp" />

    <Button
        android:id="@+id/call_button"
        android:layout_width="193dp"
        android:layout_height="46dp"
        android:onClick="callPerson[MainActivity]"
        android:text="call"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="113dp" />
</android.support.constraint.ConstraintLayout>
